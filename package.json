{
  "name": "@verifiedinc/repository-one-click-customer-starter-app",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "compile": "npm run build",
    "dev": "NODE_ENV=development next dev -p 3061",
    "build": "next build",
    "start": "NODE_ENV=production NODE_OPTIONS='-r @newrelic/next' node server.js",
    "lint": "npm run eslint && npm run prettier:check && npm run typecheck",
    "eslint": "eslint .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "typecheck": "tsc -b",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test npm run jest",
    "test:watch": "jest --watch",
    "jest": "jest",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev"
  },
  "engines": {
    "node": ">= 20.10.0"
  },
  "dependencies": {
    "@dotlottie/react-player": "^1.6.19",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@prisma/client": "^5.20.0",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "@verifiedinc/shared-ui-elements": "^0.8.12",
    "framer-motion": "^11.11.0",
    "lodash": "^4.17.21",
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.1",
    "ts-node": "^10.9.2",
    "twilio": "^5.3.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@mona-health/react-input-mask": "^3.0.3",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "libphonenumber-js": "^1.11.11",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-imask": "^7.6.1",
    "typescript": "^5",
    "uuid": "^10.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,html,md,json,yml}": "prettier --write"
  }
}
